<%= simple_form_for(@producer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :website %>
    <%= f.input :profile_url %>
    <%= f.input :phone %>
    <%= f.input :fax %>
    <%= f.input :location %>
    <%= f.input :extra %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
