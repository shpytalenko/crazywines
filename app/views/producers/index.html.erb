<h1>Listing producers</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Website</th>
      <th>Profile url</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Location</th>
      <th>Extra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @producers.each do |producer| %>
      <tr>
        <td><%= producer.title %></td>
        <td><%= producer.website %></td>
        <td><%= producer.profile_url %></td>
        <td><%= producer.phone %></td>
        <td><%= producer.fax %></td>
        <td><%= producer.location %></td>
        <td><%= producer.extra %></td>
        <td><%= link_to 'Show', producer %></td>
        <td><%= link_to 'Edit', edit_producer_path(producer) %></td>
        <td><%= link_to 'Destroy', producer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Producer', new_producer_path %>
