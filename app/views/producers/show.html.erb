<%- model_class = Producer -%>
<div class="page-header">
  <h1><%=t '.title', :default => Producer %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:title) %></dt>
      <dd><pre class="prettyprint"><%= @producer.title %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:website) %></dt>
      <dd><pre class="prettyprint"><%= @producer.website %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:profile_url) %></dt>
      <dd><pre class="prettyprint"><%= @producer.profile_url %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone) %></dt>
      <dd><pre class="prettyprint"><%= @producer.phone %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fax) %></dt>
      <dd><pre class="prettyprint"><%= @producer.fax %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:location) %></dt>
      <dd><pre class="prettyprint"><%= @producer.location %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:extra) %></dt>
      <dd><pre class="prettyprint"><%= @producer.extra %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              producers_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_producer_path(@producer), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              producer_path(@producer),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

